{
  "name": "boop",
  "module": "index.ts",
  "type": "module",
  "version": "1.0.6",
  "description": "CLI application with subcommands",
  "main": "dist/index.js",
  "bin": {
    "boop": "./dist/index.js"
  },
  "scripts": {
    "build": "bun run build:linux && bun run build:windows && bun run build:mac",
    "build:linux": "bun build src/index.ts --compile --outfile dist/boop-linux --target=bun-linux-x64 --env=BUILD_* --minify",
    "build:windows": "bun build src/index.ts --compile --outfile dist/boop-windows --target=bun-windows-x64 --env=BUILD_* --minify",
    "build:mac": "bun build src/index.ts --compile --outfile dist/boop-mac --target=bun-darwin-arm64 --env=BUILD_* --minify",
    "start": "node dist/index.js",
    "increment": "bumpp"
  },
  "dependencies": {
    "ansie": "^1.0.4",
    "commander": "^13.1.0",
    "deepmerge-ts": "^7.1.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inquirer": "^9.2.15",
    "luxon": "^3.5.0",
    "mkcert": "^3.2.0",
    "open": "^10.1.0",
    "openai": "^4.86.1",
    "ora": "^8.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.0",
    "@types/ora": "^3.2.0",
    "bumpp": "^10.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
